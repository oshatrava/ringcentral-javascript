{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "eslint-plugin",
    "import",
    "ringcentral"
  ],
  "extends": [
    "plugin:eslint-plugin/recommended"
  ]
}
